{
    "name": "YT AI Summarizer",
    "description": "YT AI Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":[
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions":[
        "*://*.youtube.com/*",
        "http://127.0.0.1:5000/",
        "http://localhost:5000/",
        "http://127.0.0.1:5001/",
        "http://localhost:5001/"
    ],
    "content_scripts":[
        {
            "matches": ["*://*.youtube.com/*"],
            "js":["content.js"]
        }
        
    ],
    "background":
        {
        "service_worker":"background.js"
        }
    ,
    "action": {
      "default_popup":"YT_AI.html",
      "default_icon": "YT_extension.png"
    }
  }